<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*$py.class;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping pattern="LICENSE" type="AUTO_DETECTED" />
      <mapping pattern="COPYRIGHT" type="AUTO_DETECTED" />
      <mapping ext="doctest" type="PLAIN_TEXT" />
      <mapping ext="po" type="PLAIN_TEXT" />
      <removed_mapping ext="ignore" approved="true" type="IgnoreLang file" />
    </extensionMap>
  </component>
</application>